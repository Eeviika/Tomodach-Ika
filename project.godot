config_version=5

[application]

config/name="Tomodach-Ika"
config/version="0.0.2"
run/main_scene="res://scenes/scr_title.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="IkaMakesGames/Tomodach_ika"
config/project_settings_override="user://overrides.cfg"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://assets/icons/default.png"
config/windows_native_icon="res://assets/icons/default.ico"

[autoload]

